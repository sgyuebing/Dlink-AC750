
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option channel '36'
	option band '5g'
	option htmode 'VHT80'
	option cell_density '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/10180000.wmac'
	option channel '1'
	option band '2g'
	option htmode 'HT40'
	option cell_density '0'
	option country 'CN'
	option mu_beamformer '1'
	option noscan '1'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option encryption 'none'
	option iw_qos_map_set 'none'
	option ssid 'AC750'

config wifi-iface 'trm_uplink2'
	option device 'radio1'
	option mode 'sta'
	option network 'trm_wwan'
	option ssid '苏观-Hotel-2G'
	option encryption 'psk-mixed+ccmp'
	option key '05365265777'
	option disabled '0'

config wifi-iface 'trm_uplink3'
	option device 'radio1'
	option mode 'sta'
	option network 'trm_wwan'
	option ssid 'BI4PCS'
	option encryption 'psk2+ccmp'
	option key 'a88888888'
	option disabled '1'

