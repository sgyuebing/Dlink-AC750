
config init
	option stdout '1'
	option stderr '1'
	option respawn '1'
	option user 'root'
	option group 'root'

config conf 'common'
	option server_addr '127.0.0.1'
	option server_port '7000'
	option tls_enable 'false'
        option login_fail_exit 'false'

config conf 'ssh'
	option type 'tcp'
	option local_ip '127.0.0.1'
	option local_port '22'
	option remote_port '6000'

